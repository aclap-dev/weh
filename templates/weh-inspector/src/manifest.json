{
    "manifest_version": 2
    , "name": "Weh Inspector"
    , "default_locale": "en_US"
    , "version": "1.0"
    , "version_name": "1.0"
    , "author": "Michel Gutierrez"
    , "background": {
        "scripts": [
            "background/default-prefs.js",
            "background/main.js"
        ]
        , "persistent": true
    }
    , "icons": {
        "32": "content/images/icon-32.png"
        , "40": "content/images/icon-40.png"
        , "48": "content/images/icon-48.png"
        , "128": "content/images/icon-128.png"
    }
    , "permissions": [
        "tabs",
        "contextMenus",
        "management"
    ]
}

